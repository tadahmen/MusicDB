<h1>Available Songs</h1>

<% @songs.each do |song| %>
  <article class="song">
    <h2><%= link_to song.name, song %></h2>
    <% if song.good_for_mood then %>
      <p> "mood booster :)" </p>
    <% else %>
      <p> "mood looser :(" </p>
    <% end %>
  </article>
<% end %>

<h3><%= link_to "Add new song", new_song_path %><h3>
<h3><%= link_to "Artist index", artists_path %><h3>
